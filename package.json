{
  "name": "cv-alex-kobylinski",
  "version": "5.1.1",
  "description": "Simple production ready boilerplate for React, Webpack (SASS and React hot reloading), tests and coverage.",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "jest",
    "coverage": "jest --coverage",
    "dist": "cross-env NODE_ENV=production webpack -p --progress",
    "postinstall": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/slantz/cv.git"
  },
  "engines": {
    "node": "5",
    "npm": "3"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "production",
    "sass",
    "hot",
    "reload"
  ],
  "author": "Alex Kobylinski <call@kblnsk.me> (http://kblnsk.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slantz/cv/issues"
  },
  "homepage": "https://github.com/slantz/cv",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.25.0",
    "classnames": "^2.2.5",
    "compression": "1.7.0",
    "cross-env": "5.0.2",
    "dotenv": "4.0.0",
    "ejs": "2.5.7",
    "express": "^4.15.3",
    "express-ejs-layouts": "2.3.1",
    "flexboxgrid": "^6.3.1",
    "lodash": "^4.17.4",
    "pixi.js": "^4.5.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flexbox-grid": "^1.1.3",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "validator": "^8.0.0",
    "webpack-parallel-uglify-plugin": "0.4.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "autoprefixer-loader": "3.2.0",
    "babel-jest": "20.0.3",
    "coveralls": "2.13.1",
    "jest-cli": "20.0.4",
    "react-addons-test-utils": "15.6.0",
    "webpack-dev-server": "2.6.1",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.1.0",
    "node-sass": "^4.5.3",
    "npm-install-webpack-plugin": "^4.0.4",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "redbox-react": "^1.5.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-validator": "^3.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": [
      "test.js",
      "test.jsx"
    ],
    "testDirectoryName": "client"
  }
}
